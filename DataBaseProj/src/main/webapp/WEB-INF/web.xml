<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
  	<display-name>Controller</display-name>
    <servlet-name>Controller</servlet-name>
    <servlet-class>Pack.Controller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Action</display-name>
    <servlet-name>Action</servlet-name>
    <servlet-class>Pack.Action</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Action</servlet-name>
    <url-pattern>/Action</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddFacultyProject</display-name>
    <servlet-name>AddFacultyProject</servlet-name>
    <servlet-class>Pack.AddFacultyProject</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddFacultyProject</servlet-name>
    <url-pattern>/AddFacultyProject</url-pattern>
  </servlet-mapping>
</web-app>